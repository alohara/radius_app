<li id="<%= feed_item.id %>">
  <span class="user">
    <%= feed_item.interest %>
    <%= feed_item.subcity %>
    <%= feed_item.zipcode %>
  </span>
  <span class="content">
    <%= auto_link(wrap(feed_item.content),:html => { :target => '_blank' }) %>
  </span>
  <span class="timestamp">
    <%= link_to feed_item.user.radius_name, feed_item.user %>
    Posted <%= time_ago_in_words(feed_item.created_at) %> ago.
	<%= feed_item.latitude %>
	<%= feed_item.longitude %>
  </span>
  <% if current_user?(feed_item.user) %>
  <%= link_to "delete", feed_item, method: :delete,
                                           confirm: "You sure, Yo?",	   
										   title: feed_item.content %>
  <% end %>
</li>